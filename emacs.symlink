;;; -*- mode: emacs-lisp; -*-

(load "~/.emacs-config/.general-configuration")
(load "~/.emacs-config/.install-packages")
(dumb-jump-mode)
(global-set-key (kbd "C-c .") 'dumb-jump-go)
(global-set-key (kbd "C-c ,") 'dumb-jump-quick-look)
(global-set-key (kbd "C-c M-.") 'dumb-jump-back)
(global-set-key (kbd "C-c g") 'writegood-mode)
(load-theme 'zenburn t)
(load "~/.emacs-config/.flycheck-init")
(load "~/.emacs-config/.multiple-cursors-init")
(load "~/.emacs-config/.linum-init")
(load "~/.emacs-config/.ido-init")
