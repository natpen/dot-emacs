(load "~/.emacs-config/.projectile-init")
(load "~/.emacs-config/.smex-init")
(load "~/.emacs-config/.magit-init")
(load "~/.emacs-config/.highlight-indentation")
(load "~/.emacs-config/.auto-complete")
(load "~/.emacs-config/.writegood-init")

;; language-specific
(load "~/.emacs-config/.golang-settings")
(load "~/.emacs-config/.javascript-settings")
(load "~/.emacs-config/.python-settings")
(load "~/.emacs-config/.rust-settings")

